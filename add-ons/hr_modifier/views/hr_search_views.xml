<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <record id="customer_map_view" model="ir.ui.view">
      <field name="name">customer.map.view</field>
      <field name="model">res.partner</field>
      <field name="arch" type="xml">
        <map latitude="partner_latitude" longitude="partner_longitude">
          <field name="name"/>
        </map>
      </field>
    </record>

    <record id="action_hr_advanced_search" model="ir.actions.act_window">
      <field name="name">Advanced search</field>
      <field name="res_model">hr.employee</field>
      <field name="view_mode">form</field>
      <field name="view_id" ref="hr.view_employee_form"/>
      <field name="target">new</field>
    </record>

  </data>
</odoo>